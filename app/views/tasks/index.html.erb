<h1>Tasks</h1>
<%=link_to 'New Register', url_for(action: :new), class: 'btn btn-primary' %>

<%= will_paginate @tasks %>
<table class="table" id="tasks">
  <thead class="thead-light">
    <tr>
      <th>#</th>
      <th>Category</th>
      <th>Title</th>
      <th>Completed at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <%= render @tasks %>
  </tbody>
</table>

<%= will_paginate @tasks %>
